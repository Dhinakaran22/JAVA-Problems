// Reversing a number
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        // Input
        Scanner input = new Scanner(System.in);
        System.out.print("Enter the number:");
        int n = input.nextInt();

        // Intialization
        int ans = 0;

        // Condition
        while(n > 0) {
            int sum = n % 10;
            n = n / 10;
            ans = ans * 10 + sum;
        }
            System.out.println(ans);
    }
}
