import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        // Function
        factorial();
    }

    static void factorial() {
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();

        //Initialization
        int i,n=1;
        for (i = 1; i <= a; i++) {
            n = n * i;
        }
        System.out.println(n);
    }
}
